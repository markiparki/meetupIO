<html>
	<head>
		<title><%= title %></title>
		<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular.js"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular-route.js"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.0/angular-resource.js"></script>
		<script src="javascripts/angularApp.js"></script>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
		<link rel="stylesheet" href="stylesheets/style.css">
	</head>
	<body ng-app="meetupApp">
		<div id='main' class="container">
			<nav class="navbar-fluid navbar-default navbar-fixed-top">
				<div class="container">
					    <a class="navbar-brand" href="/"> meetupIO </a>
					    <p class="navbar-text"> first angular try</p>
					    <p class="navbar-right navbar-text" ng-hide="authenticated"><a href="/auth/facebook">Login with facebook</a></p>
					    <p class="navbar-right navbar-text" ng-show="authenticated"><a href="/auth/logout">Logout</a></p>
					    <p class="navbar-right navbar-text" ng-show="authenticated">Logged in as {{currentUser}}</p>		
				</div>
			</nav>
			<div class="col-md-offset-2 col-md-8">
				<div ng-view></div>
			</div>
		</div>
	</body>
</html>